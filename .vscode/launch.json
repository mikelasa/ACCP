{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug rs-sensor-control",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/librealsense/build/Debug/rs-sensor-control",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/librealsense/build/Debug",
            "environment": [
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "${workspaceFolder}/librealsense/build/Debug:${workspaceFolder}/librealsense/build:${env:LD_LIBRARY_PATH}"
                }
            ],
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
